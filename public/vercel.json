{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value":  "ALLOW-FROM https://app.storyblok.com"
          },
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors https://app.storyblok.com"
          }
        ]
      }
    ]
  }
  